{
  "name": "express-ws",
  "version": "1.0.0",
  "description": "Test Driven Development with express",
  "main": "app.js",
  "scripts": {
    "start": "node app",
    "test": "jest --watch"
  },
  "author": "Greg Wagner",
  "license": "ISC",
  "dependencies": {
    "express": "^4.17.1"
  },
  "devDependencies": {
    "eslint": "^7.20.0",
    "jest": "^26.6.3",
    "supertest": "^6.1.3"
  },
  "eslintConfig ": {
      "parserOptions": {
          "ecmaVersion": 6
      }.
      "extends": [
          "eslint: recommended"
      ],
      "env": {
          "node": true,
          "es6": true,
          "jest": true
      },
      "rules": {
          "semi": "warn",
          "quotes": ["warn", "single"]
      }
  }
