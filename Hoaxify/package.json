{
  "name": "express-ws",
  "version": "1.0.0",
  "description": "Test Driven Development with express",
  "main": "app.js",
  "scripts": {
    "start": "node app",
    "test": "jest --watch"
  },
  "author": "Greg Wagner",
  "license": "ISC",
  "dependencies": {
    "eslint-config-prettier": "^8.0.0",
    "eslint-plugin-prettier": "^3.3.1",
    "express": "^4.17.1",
    "prettier": "^2.2.1",
    "save-dev": "0.0.1-security"
  },
  "devDependencies": {
    "eslint": "^7.20.0",
    "jest": "^26.6.3",
    "supertest": "^6.1.3"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 2018
    },
    "extends": [
      "eslint:recommended",
      "plugin:prettier/recommended"
    ],
    "plugins": [
      "prettier"
    ],
    "env": {
      "node": true,
      "es6": true,
      "jest": true
    },
    "rules": {
      "eqeqeq": "warn",
      "prettier/prettier": "warn"
    }
  },
  "prettier": {
    "singleQuote": true,
    "printWidth": 120
  }
}
